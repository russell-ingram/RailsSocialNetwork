<% case name %>
<% when :search_results_page_s_confirm_connect %>

	<div id="connect<%= index %>" class="addIntentionModal the_<%= name %>_modal confirmConnectModal" style="display:none;">
	  <div class="formBox searchFormBox formBoxModal confirmModalBox connectConfirm">
	    <div class="formHeader">CONFIRM CONNECT</div>
	    <div class="modalBody">
	      <div class="modalIntentions">
	        <div class="myConnProfilePic">
	          <a href="/profile/<%= user.id %>">
	            <% if user.profile_pic_url? %>
	              <img src="<%= user.profile_pic_url %>">
	            <% elsif user.linkedin_pic_url? %>
	                <img src="<%= user.linkedin_pic_url %>">
	            <% else %>
	              <%= image_tag("avatar-icons/avatar_0"+rand(1..9).to_s+".svg")  %>
	            <% end %>
	          </a>
	        </div>
	      </div>
	      <div class="modalIntentionsRight">
	        <div class="deleteUserModalInfo">
	          Are you sure you would like to connect with <%= user.full_name %>?
	        </div>
	        <div class="field-area">
	          <textarea placeholder="You may include a message in your request" class='connectMessage'></textarea>
	        </div>
	        <div class="modalDoneButton confirmConnectButton" data="<%= user.id %>" data-index="<%= index %>">
	          <a href="#close-modal" rel="modal:close">
	          CONFIRM
	          </a>
	        </div>
	      </div>
	    </div>
	  </div>
	</div>

<% when :search_results_page_s_confirm_connect_w_intentions %>

    <div id="connectAnon<%= index %>" class="addIntentionModal the_<%= name %>_modal confirmConnectModal" style="display:none;">
